filters:
  and:
    - '!file.hasTag("entry_point")'
    - file.ext == "md"
    - file.hasTag("math")
    - file.hasTag("concept")
views:
  - type: table
    name: Table
    order:
      - file.name
      - keywords
      - tags
      - file.ctime
    sort: []
    columnSize:
      file.name: 413
      note.tags: 320
      file.ctime: 222
