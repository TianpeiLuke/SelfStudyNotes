filters:
  and:
    - file.hasTag("concept")
    - '!file.hasTag("entry_point")'
    - or:
        - file.hasTag("optimization")
        - file.hasTag("numerical_methods")
views:
  - type: table
    name: Table
    order:
      - file.name
      - keywords
      - tags
      - file.ctime
    columnSize:
      file.name: 421
      note.tags: 412
